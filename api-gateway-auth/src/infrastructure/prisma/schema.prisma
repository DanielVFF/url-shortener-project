generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  user_id        String   @id @default(uuid()) @db.Uuid
  name        String
  email       String   @unique
  password    String
  phone_number String?
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
